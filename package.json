{
  "name": "surf-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "prisma generate && next build",
    "start": "next start -p ${PORT:-3333}",
    "lint": "next lint",
    "db:push": "npx prisma db push",
    "sync": "ts-node -r tsconfig-paths/register ./src/services/surfLine/useCase/getRegionBeaches.ts",
    "forcast:get": "ts-node -r tsconfig-paths/register ./src/services/surfLine/useCase/getForcasts.ts",
    "seed": "ts-node ./src/prisma/scripts/addUsers.ts",
    "add-records": "ts-node -r tsconfig-paths/register ./src/prisma/scripts/import-records.ts",
    "db:migrate:sqlite": "ts-node ./src/prisma/scripts/migrate-data-from-postgres.ts",
    "delete": "ts-node ./src/prisma/scripts/removeActivites.ts",
    "deploy": "pm2 restart surf-tracker || pm2 start ecosystem.config.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@nextui-org/react": "^1.0.0-beta.12",
    "@prisma/client": "5",
    "@types/bluebird": "^3.5.38",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@visx/group": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/text": "^3.3.0",
    "@visx/wordcloud": "^3.3.0",
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "echarts": "^5.4.2",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-iconly": "^2.2.10",
    "react-select": "^5.7.3",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.1",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.23",
    "prisma": "5",
    "tailwindcss": "3.3.2"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  }
}
